{
  "mcpServers": {
    "typescript": {
      "command": "npx",
      "args": [
        "typescript-mcp"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "readability": {
      "command": "npx",
      "args": [
        "-y",
        "@mizchi/readability",
        "--mcp"
      ]
    }
  }
}